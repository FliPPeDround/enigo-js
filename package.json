{
  "name": "@enigo-js/core",
  "version": "0.0.0",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "dev": "concurrently  \"nodemon\" \"tsc -w\"",
    "build": "napi build --platform --release --js rust-module.js --dts rust-module.d.ts -c binaries.json",
    "build:debug": "napi build --platform --js rust-module.js --dts rust-module.d.ts -c binaries.json",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version",
    "play": "cd playground && pnpm run dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.12.2",
    "@napi-rs/cli": "^2.18.0",
    "ava": "^6.0.1",
    "concurrently": "^8.2.2",
    "eslint": "^9.0.0",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.4"
  },
  "ava": {
    "timeout": "3m"
  }
}
